<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<StackPane id="startPane" fx:id="startPane" style="-fx-background-color: #081820;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.StartController">
   <children>
      <VBox alignment="CENTER" prefHeight="342.0" prefWidth="750.0" StackPane.alignment="CENTER">
         <children>
            <ImageView fx:id="titleImage" fitHeight="314.0" fitWidth="667.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fx:id="bonfire" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="enterLabel" style="-fx-font-family: &quot;Early GameBoy&quot;; -fx-text-fill: #e0f8d0; -fx-font-size: 10;" text="Enter your Hero's name:">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="nameField" maxWidth="-Infinity" onKeyTyped="#setName" prefHeight="51.0" prefWidth="400.0" style="-fx-font-family: &quot;Early GameBoy&quot;; -fx-font-size: 10;" />
            <HBox alignment="CENTER" prefHeight="7.0" prefWidth="750.0">
               <children>
                  <Button fx:id="startButton" mnemonicParsing="false" onAction="#startClick" style="-fx-font-family: &quot;Early GameBoy&quot;; -fx-font-size: 10;" text="Start">
                     <HBox.margin>
                        <Insets left="10.0" right="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="quitButton" mnemonicParsing="false" onAction="#quitClick" style="-fx-font-family: &quot;Early GameBoy&quot;; -fx-font-size: 10;" text="Quit" />
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
